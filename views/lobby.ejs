    $("#home").click(function () {
        window.location.href = "http://localhost:3000/lobby/" + <%- pseudo %>
    });

    $("#gw").click(function () {
        window.location.href = "http://localhost:3000/lobby/" + <%- pseudo %>
    });

    $("#debate").click(function () {
        window.location.href = "http://localhost:3000/lobby/" + <%- pseudo %>
    });
</script>

